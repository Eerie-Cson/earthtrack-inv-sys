{
  "name": "shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project shared --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "assets": ["src/proto/*.proto"],
        "additionalCommand": "npx grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=nestJs=true --proto_path=./src/proto ./src/proto/*.proto"
      }
    }
  }
}
